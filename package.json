{
  "name": "express-mvc-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "console": "node --experimental-repl-await ./bin/console",
    "lint": "npx eslint . & npx pug-lint ./views",
    "test": "jest --runInBand",
    "db:drop": "npx sequelize-cli db:drop --debug",
    "db:create": "npx sequelize-cli db:create --debug",
    "db:migrate": "npx sequelize-cli db:migrate --debug",
    "db:seed": "npx sequelize-cli db:seed:all --debug",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "routes": "DEBUG= bin/routes"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/register": "^7.15.3",
    "bcrypt": "^5.0.1",
    "bufferutil": "^4.0.3",
    "connect-session-sequelize": "^7.1.2",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.11.0",
    "debug": "^4.3.1",
    "express": "~4.17.2",
    "express-flash-message": "^2.1.0",
    "express-routemap": "^1.6.0",
    "express-session": "1.17.2",
    "helmet": "^5.0.2",
    "http-errors": "~2.0.0",
    "i18n": "^0.14.1",
    "method-override": "^3.0.0",
    "morgan": "~1.10.0",
    "passport": "0.5.2",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.3",
    "pg-native": "^3.0.0",
    "pluralize": "^8.0.0",
    "pug": "3.0.2",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.2.0",
    "utf-8-validate": "^5.0.5"
  },
  "devDependencies": {
    "dotenv": "^15.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-pug": "^1.2.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.12",
    "pug-lint": "^2.6.0",
    "supertest": "^6.1.6"
  }
}
